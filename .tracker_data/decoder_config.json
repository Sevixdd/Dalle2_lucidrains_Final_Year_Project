{
    "decoder": {
        "unets": [
            {
                "dim": 128,
                "cond_dim": 128,
                "image_embed_dim": 768,
                "text_embed_dim": 768,
                "cond_on_text_encodings": true,
                "channels": 3,
                "dim_mults": [1, 2, 4, 8],
                "num_resnet_blocks": 2,
                "attn_heads": 8,
                "attn_dim_head": 64,
                "sparse_attn": true,
                "memory_efficient": true,
                "self_attn": [false, false,false ,false ]
            }

        ],
        "clip": {
            "make": "openai",
            "model": "ViT-L/14"
        },
        "image_sizes": [128],
        "channels": 3,
        "timesteps": 1000,
        "loss_type": "l2",
        "beta_schedule": ["cosine"],
        "learned_variance": true,
        "text_cond_drop_prob": 0.1,
        "image_cond_drop_prob": 0.1
    },
    "data": {
        "webdataset_base_url": "/home/sevi/datasets/dataset/{}.tar",
        "img_embeddings_url:": "/home/sevi/datasets/reorder",
        "text_embeddings_url:": "/home/sevi/datasets/embedings/text_emb",
        "num_workers": 2,
        "batch_size": 4,
        "start_shard": 0,
        "cond_scale": [3.5, 1.0],
        "end_shard": 591,
        "shard_width": 5,
        "index_width": 3,
        "splits": {
            "train": 0.75,
            "val": 0.15,
            "test": 0.1
        },
        "shuffle_train": true,
        "preprocessing": {
            "RandomResizedCrop": {
                "size": [224, 224],
                "scale": [0.75, 1.0],
                "ratio": [1.0, 1.0]
            },
            "ToTensor": true
        }
    },
    "train": {
        "epochs": 1,
        "lr": 1.1e-4,
        "wd": 0.01,
        "max_grad_norm": 0.5,
        "save_every_n_samples": 110000,
        "n_sample_images": 10,
        "cond_scale": [3.5, 1.0],
        "device": "cuda",
        "epoch_samples": 460000,
        "validation_samples": 20000,
        "use_ema": true,
        "ema_beta": 0.99,
        "save_latest": true,
        "save_best": true,
        "amp": false,
        "unet_training_mask": [true]
    },
    "evaluate": {
        "n_evaluation_samples": 10,
        "FID": {
            "feature": 64
        },
        "IS": {
            "feature": 64,
            "splits": 10
        },
        "KID": {
            "feature": 64,
            "subset_size": 2
        },
        "LPIPS": {
            "net_type": "vgg",
            "reduction": "mean"
        }
    },
    "tracker": {
        "overwrite_data_path": true,


        "log": {
            "log_type": "wandb",

            "wandb_entity": "Sevixdd",
            "wandb_project": "decoder_train",
            "auto_resume": false,

            "verbose": true
        },

        "load": {
            "load_from": null,
            "only_auto_resume": true,
            "file_path": "latest.pth"
        },

       "save": [{
            "save_to": "local",
            "save_latest_to": "latest.pth",
               "save_best_to": "best_decoder.pth"
        }]
    }
}